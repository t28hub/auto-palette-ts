{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/index.ts"],
  "paths": {
    "auto-palette": ["src/index.ts"],
    "@internal/*": ["src/*"]
  },
  "project": ["**/*.ts"],
  "ignore": ["**/*.d.ts"],
  "ignoreBinaries": ["only-allow"],
  "ignoreDependencies": ["conventional-changelog-conventionalcommits"],
  "vite": {
    "config": ["vite.config.ts"]
  }
}
